{
  "name": "audiomoth-visualizer",
  "version": "1.2.0",
  "description": "Pipeline for spectrograms and birdnet detection for audiomoth sessions.",
  "main": "./index.mjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"**/*.{js,mjs,json,md}\"",
    "release": "release-it"
  },
  "keywords": [
    "audiomoth",
    "spectrogram",
    "birdnet"
  ],
  "author": "Ivo Bathke",
  "bin": {
    "audiomoth-visualizer": "./index.mjs"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivoba/audiomoth-visualizer.git"
  },
  "dependencies": {
    "@picocss/pico": "^1.3.3",
    "csv-reader": "^1.0.8",
    "date-fns": "^2.28.0",
    "perfundo": "^4.0.4",
    "posthtml": "^0.16.5",
    "posthtml-content": "0.0.3",
    "posthtml-expressions": "^1.7.1",
    "sass": "^1.36.0",
    "staticmaps": "^1.8.0",
    "zx": "^6.1.0"
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "release-it": "^14.12.4",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "semantic-release": "^19.0.2"
  },
  "bugs": {
    "url": "https://github.com/ivoba/audiomoth-visualizer/issues"
  },
  "homepage": "https://github.com/ivoba/audiomoth-visualizer#readme",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "*.mjs",
    "lib",
    "views"
  ],
  "release": {
    "requireCleanWorkingDir": false,
    "requireUpstream": false,
    "git": {
      "commitMessage": "chore: release ${version}"
    },
    "npm": {
      "publish": false
    },
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ]
        }
      ]
    ]
  }
}
